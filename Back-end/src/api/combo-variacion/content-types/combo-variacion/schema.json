{
  "kind": "collectionType",
  "collectionName": "combos_variaciones",
  "info": {
    "singularName": "combo-variacion",
    "pluralName": "combos-variaciones",
    "displayName": "ComboVariacion"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "stock": {
      "type": "integer",
      "required": true
    },
    "talle": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "XS",
        "S",
        "M",
        "L",
        "XL",
        "XXL",
        "XXXL"
      ]
    },
    "combo": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::combo.combo",
      "inversedBy": "combos-variaciones"
    },
    "detalle_carritos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detalle-carrito.detalle-carrito",
      "mappedBy": "combo_variacion"
    },
    "detalle_ventas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::detalle-venta.detalle-venta",
      "mappedBy": "combo_variacion"
    }
  }
}
