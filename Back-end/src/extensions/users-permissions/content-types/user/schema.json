{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "direccion_usuarios": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::direccion-usuario.direccion-usuario",
      "mappedBy": "users_permissions_user"
    },
    "carritos": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::carrito.carrito",
      "mappedBy": "users_permissions_user"
    },
    "ventas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::venta.venta",
      "mappedBy": "users_permissions_user"
    },
    "newsletter": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::newsletter.newsletter",
      "mappedBy": "users_permissions_user"
    },
    "wishlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "users_permissions_user"
    },
    "resenas": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::resena.resena",
      "mappedBy": "users_permissions_user"
    }
  }
}
